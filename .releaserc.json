{
  "branches": [
    "main",
    {
      "name": "feat/*",
      "prerelease": "feat"
    },
    {
      "name": "style/*",
      "prerelease": "style"
    },
    {
      "name": "fix/*",
      "prerelease": "fix"
    },
    {
      "name": "chore/*",
      "prerelease": "chore"
    },
    {
      "name": "refactor/*",
      "prerelease": "refactor"
    },
    {
      "name": "test/*",
      "prerelease": "test"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator"
  ]
}